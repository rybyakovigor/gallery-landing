{
  "name": "gallery-landing",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev & tcm --watch src",
    "start": "next start",
    "build": "next build",
    "check:all": "concurrently \"npm:check:*(!all)\"",
    "check:all:pre-commit": "concurrently -r \"npx lint-staged --relative\" \"npm:check:types\"",
    "check:lint": "eslint src",
    "check:prettier": "prettier --check src",
    "check:stylelint": "stylelint src/**/*.css",
    "check:types": "tsc --noEmit",
    "fix:all": "concurrently \"npm:fix:*(!all)\"",
    "fix:lint": "eslint src --fix",
    "fix:prettier": "prettier --write src",
    "fix:stylelint": "stylelint src/**/*.css --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18",
    "react-dom": "18",
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@next/eslint-plugin-next": "14.1.0",
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "0.24.0",
    "husky": "8.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-recess-order": "4.6.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "typed-css-modules": "0.9.1",
    "typescript": "5"
  }
}
